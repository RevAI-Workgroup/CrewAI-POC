{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:production": "tsc -b && vite build --mode production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "docker:build": "docker build -t crewai-frontend .",
    "docker:run": "docker run -p 3000:80 crewai-frontend",
    "analyze": "npm run build && npx vite-bundle-analyzer dist"
  },
  "dependencies": {
