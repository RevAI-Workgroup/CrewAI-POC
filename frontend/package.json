    "react": "^18.3.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.56.4",
    "react-router-dom": "^7.6.1",
    "recharts": "^2.15.3",
